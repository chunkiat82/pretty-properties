{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "strict": 0,
    "indent": 2,
    "mocha/no-exclusive-tests": "error"
  },
  "plugins": [
    "react",
    "import",
    "mocha"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint"
}